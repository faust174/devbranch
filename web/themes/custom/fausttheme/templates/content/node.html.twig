
<article {{ bem("node") }}>
  {{ title_prefix }}
  {% if not page %}
    {% include "@atoms/text/headings/_heading.twig" with {
      "heading_level": 2,
      "heading": label,
      "heading_url": url,
      "heading_link_attributes": {
        'rel': 'bookmark'
      },
    } %}
  {% endif %}
  {{ title_suffix }}

  <div{{ content_attributes }}>
    {{ content.field_image_paragraph }}
    {{ content.field_tags }}
  </div>

</article>


